{
    "name": "Test Extension",
    "description": "This extension will help me to test",
    "version": "1.0",
    "manifest_version": 2,
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "My Test Extention" 
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "bookmarks",
        "background",
        "notifications"
    ]
}